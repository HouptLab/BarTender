#include "BarObjects.hpp"ItemType::ItemType(unsigned long num) {	if (num == 0) {		sprintf(name,"Food");		sprintf(tag,"F");	}		else if (num == 1) {		sprintf(name,"Water");		sprintf(tag,"W");	}	else if (num == 2) {		sprintf(name,"Test");		sprintf(tag,"T");	}	else {			sprintf(name,"Item %ld",num);		sprintf(tag,"%c",(char)num + 'a');	}}Boolean ItemType::CheckTag(char *string) {	// format is always ExptTAG-RATID-ITEMTAG			return(FALSE);}